   "source": [
    "# Fibonacci\n",
    "\n",
    "numero = int(input('Escolha um numero:'))\n",
    "resultado = int(numero)\n",
    "\n",
    "while(numero > 1):\n",
    "    resultado = resultado * (numero - 1)\n",
    "    numero -= 1;\n",
    "print(f\"O resultado final é: {resultado}\")"
   ]
